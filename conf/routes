# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET		/tech-eval/introduction		controllers.Application.index()
GET     /tech-eval/about            controllers.Application.about()
GET     /tech-eval/architecture     controllers.Application.architecture()
GET     /tech-eval/features         controllers.Application.features()
GET		/tech-eval/activator		controllers.Application.activator()
GET     /tech-eval/dir              controllers.Application.dir()
GET     /tech-eval/actions          controllers.Application.actions()
GET     /tech-eval/views            controllers.Application.views()
GET     /tech-eval/routes           controllers.Application.routes()
GET     /tech-eval/cons             controllers.Application.cons()
GET     /tech-eval/pros             controllers.Application.pros()
GET     /tech-eval/background       controllers.Application.background()
GET     /tech-eval/comparison       controllers.Application.comparison()
GET		/tech-eval/qs               controllers.Application.questions()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
